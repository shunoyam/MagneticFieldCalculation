set title "By generating mode"
set title offset 0,-1
set xl "{/Symbol q} [rad]"
set yl "B_{y} [T]"
set autoscale
set  yl offset -1.8,0
set title font "Arial,20"
set xl font "Arial,20"
set yl font "Arial,20"
set xtics font "Arial,15"
set ytics font "Arial,15"
set key font "Arial,15"
set border lw 2
set size ratio .6
set key bottom center
p "svd.dat"  u 1:2 title "SVD" w l lw 2 lc rgb "blue"
rep "svd.dat"  u 1:3 title "simple" w l lw 2 lc rgb "red"
set term png
set output ".png"
rep







